{
  "name": "react-starter-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.8.0",
  "author": "Selemondev",
  "license": "MIT",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "vitest",
    "tailwind",
    "template",
    "starter",
    "eslint",
    "prettier",
    "husky",
    "commit-lint",
    "react-redux",
    "prettier"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "coverage": "vitest run --coverage",
