    "@reduxjs/toolkit": "^1.9.7",
    "@szhsin/react-accordion": "^1.2.4",
    "@uidotdev/usehooks": "^2.3.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "html-react-parser": "^5.1.15",
    "jsonwebtoken": "^9.0.2",
